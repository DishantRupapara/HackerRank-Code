#!/bin/python3

import os
import sys

#
# Complete the 'biggerIsGreater' function below.
#
# The function is expected to return a STRING.
# The function accepts STRING w as parameter.
#

def biggerIsGreater(w):
    # Convert string to list for easy manipulation
    w = list(w)
    n = len(w)
    
    # Step 1: Find the pivot (rightmost char which is smaller than its next char)
    i = n - 2
    while i >= 0 and w[i] >= w[i + 1]:
        i -= 1
    
    if i == -1:
        return "no answer"
    
    # Step 2: Find the rightmost character that is greater than pivot
    j = n - 1
    while w[j] <= w[i]:
        j -= 1
    
    # Step 3: Swap pivot with this character
    w[i], w[j] = w[j], w[i]
    
    # Step 4: Reverse suffix (everything after position i)
    w[i+1:] = reversed(w[i+1:])
    
    return ''.join(w)


if __name__ == '__main__':
    fptr = open(os.environ['OUTPUT_PATH'], 'w')

    T = int(input().strip())

    for T_itr in range(T):
        w = input().strip()

        result = biggerIsGreater(w)

        fptr.write(result + '\n')

    fptr.close()
